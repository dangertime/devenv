{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "#ffffff",
            "properties": {
              "prefix": "",
              "text": "<#C591E8>\u276F</><#69FF94>\u276F</>"
            }
          },
          {
            "type": "path",
            "style": "plain",
            "foreground": "#56B6C2",
            "properties": {
              "prefix": "",
              "style": "mixed"
            }
          },
          {
            "type": "poshgit",
            "style": "plain",
            "properties": {
              "display_status": true,
              "branch_identical_icon": "\uF14A",
              "branch_gone_icon": "\u274E",
              "prefix": "<#5FAAE8>git:(</>",
              "postfix": "<#5FAAE8>)</>"
            }
          },
          {
            "type": "exit",
            "style": "plain",
            "foreground": "#DCB977",
            "properties": {
              "prefix": "\uF119",
              "display_exit_code": false
            }
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "right",
        "segments": [
          {
            "type": "command",
            "style": "plain",
            "foreground": "#ffffff",
            "properties": {
              "shell": "bash",
              "command": "git log --pretty=format:%cr -1 || date +%H:%m:%S"
            }
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "left",
        "newline": true,
        "segments": [
            {
                "type": "exit",
                "style": "powerline",
                "foreground": "#69FF94",
                "properties": {
                    "display_exit_code": false,
                    "always_enabled": true,
                    "error_color": "red",
                    "prefix": "\u279c"
                }
            }
        ]
      }
    ]
  }
  